<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Heath Check Tests" verbose="1">

    <test name="Critical tests" parallel="true">
        <groups>
            <run>
                <include name="HealthCheck"/>
            </run>
        </groups>

        <packages>
            <package name="by.kazak.taf.apiTest.*"/>
            <package name="by.kazak.taf.uiTest.*"/>
        </packages>
    </test>
</suite>
